{%- else %}
                "Name": "",
{%- endif %}
                "Type": "CNAME",
                "TTL": 300,
                "ResourceRecords": [
                    {
                        "Value": "<INSERT_LOADBALANCER_URL>"
                    }
                ]
            }
        }
    ]
}